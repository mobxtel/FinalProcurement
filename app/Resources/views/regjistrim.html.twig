<div class="main-content " style="background-color: #F3F5F8" height="95vh">
    <div class="container-fluid  " id="viewFull">
        <div class="row" >
            <div class="col-lg-10 col-lg-offset-1">

                <div class="panel  " style="padding: 0px !important; ">
                    <div class="panel-body"  id="Register">
                        <div class="col-lg-4  col-md-4 ">
                            <div class="  imageRegister p-3 ">
                                <div class="  overlay">
                                </div>
                                <div class="content text">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-8 col-md-8 " style="padding-right:40px;padding-left:40px">
                            <div class="content py-5  registerForm ">
                                <div class="row "  style="padding-top:10px !important;">
                                    <div  class="RegisterHeading">
                                        <h3  >Rregjistrohu
                                        </h3>
                                        <p class="lead">Nese keni llogari,
                                            <a href="{{ path('identifikohu') }}"> identifikohuni!
                                            </a>
                                        </p>
                                    </div>
                                    <div class="header"  style="float:right">
                                        <div class="logo text-center">
                                            <img src="assets/img/Logo.png"  alt=" Logo" width="120px">
                                        </div>
                                    </div>
                                </div>
                                <div class="row registerForm ">
                                    {{ form_start(form) }}
                                    <div class="form-group col-md-6">
                                        {{ form_row(form.emer_biznesi,{'attr': {'class': 'form-control'}}) }}
                                    </div>
                                    <div class="form-group col-md-6">
                                        {{ form_row(form.nipt,{'attr': {'class': 'form-control','for':'Emerbiznesi'}}) }}
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group  col-md-6 ">
                                        {{ form_row(form.email,{'attr': {'class': 'form-control'}}) }}
                                    </div>
                                    <div class="form-group col-md-6">
                                        {{ form_row(form.numerTelefoni,{'attr': {'class': 'form-control'}}) }}
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="form-group  col-md-6 ">
                                        {{ form_row(form.fushe_operimi_id,{'attr': {'class': 'form-control'}}) }}
                                    </div>
                                    <div class="form-group col-md-6">
                                        {{ form_row(form.bashkia,{'attr': {'class': 'form-control'}}) }}
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group  col-md-12 ">
                                        {{ form_row(form.adresa,{'attr': {'class': 'form-control'}})}}
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="form-group  col-md-6 ">
                                        {{ form_row(form.logo,{'attr': {'class': 'form-control'}}) }}
                                    </div>
                                    <div class="form-group  col-md-6 ">
                                        {{ form_row(form.password,{'attr': {'class': 'form-control'}}) }}
                                    </div>
                                </div>
                                {{ form_row(form.submit,{'attr': {'class': ' registerBtn btn btn-primary btn-lg btn-block',
                                    'tag':'button','style':'width: 100%; padding-left:0px;background-color:#FFC107'}})}}
                                {{ form_end(form) }}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
